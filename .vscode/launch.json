        {
            "name": "(gdb) test_state_manager",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/test/test_state_manager",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/resources",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
